    </div class="test">
    <% if (dyrs.length > 0) { %>
        <% dyrs.forEach((dyrs, index) => { %>
            <div class="dyrbox">

                <h1>Eid av <%= dyrs.eier ? dyrs.eier.navn : 'Unknown' %></h1>
                <h2><%= dyrs.navn%></h2>
                <h2><%= dyrs.Serienumber%></h2>
                <h2><%= dyrs.flokk ? dyrs.flokk.navn : 'Unknown Flokk' %></h2>
                <h4><%= dyrs.dato%></h4>
                
            </div>
           
            <% }); %>
        <% } else { %>
         
        <% } %>
</body>
