<body>

    <h2>Login</h2>
    <form action="/login" method="POST">
        <div>
            <label for="epost">Email</label>
            <input type="email" name="epost" required>
        </div>
        <div>
            <label for="passord">Password</label>
            <input type="password" name="passord" required>
        </div>
        <button type="submit">Login</button>
    </form>

</body>
</html>